  CERecipe = class
    [JsonName('RowName')]
    RowName : string;
    [JsonName('RecipeName')]
    RecipeName : string;
    [JsonName('ShortDesc')]
    ShortDesc : string;
    [JsonName('RecipeType')]
    RecipeType : string;
    [JsonName('TimeToCraft')]
    TimeToCraft : FixedUInt;
    [JsonName('Tier')]
    Tier : FixedUInt;
    [JsonName('CraftXP')]
    CraftXP : FixedUInt;
    [JsonName('CraftingStations')]
    CraftingStations : FixedUInt;
    [JsonName('RequiredFuel')]
    RequiredFuel : FixedUInt;
    [JsonName('Icon')]
    Icon : string;
    [JsonName('BuildingModule')]
    BuildingModule : string;
    [JsonName('DestroyStationOnComplete')]
    DestroyStationOnComplete : boolean;
    [JsonName('RecipeItemFlags')]
    RecipeItemFlags : FixedUInt;
    [JsonName('Ingredient1ID')]
    Ingredient1ID : FixedUInt;
    [JsonName('Ingredient1Quantity')]
    Ingredient1Quantity : FixedUInt;
    [JsonName('Ingredient2ID')]
    Ingredient2ID : FixedUInt;
    [JsonName('Ingredient2Quantity')]
    Ingredient2Quantity : FixedUInt;
    [JsonName('Ingredient3ID')]
    Ingredient3ID : FixedUInt;
    [JsonName('Ingredient3Quantity')]
    Ingredient3Quantity : FixedUInt;
    [JsonName('Ingredient4ID')]
    Ingredient4ID : FixedUInt;
    [JsonName('Ingredient4Quantity')]
    Ingredient4Quantity : FixedUInt;
    [JsonName('CatalystID')]
    CatalystID : FixedUInt;
    [JsonName('Result1ID')]
    Result1ID : FixedUInt;
    [JsonName('Result1Quantity')]
    Result1Quantity : FixedUInt;
    [JsonName('Result2ID')]
    Result2ID : FixedUInt;
    [JsonName('Result2Quantity')]
    Result2Quantity : FixedUInt;
    [JsonName('Result3ID')]
    Result3ID : FixedUInt;
    [JsonName('Result3Quantity')]
    Result3Quantity : FixedUInt;
    [JsonName('Result4ID')]
    Result4ID : FixedUInt;
    [JsonName('Result4Quantity')]
    Result4Quantity : FixedUInt;
    [JsonName('Result1Weight')]
    Result1Weight : FixedUInt;
    [JsonName('Result2Weight')]
    Result2Weight : FixedUInt;
    [JsonName('Result3Weight')]
    Result3Weight : FixedUInt;
    [JsonName('Result4Weight')]
    Result4Weight : FixedUInt;
    [JsonName('ThrallRecipeFeatRequirement')]
    ThrallRecipeFeatRequirement : FixedUInt;
    [JsonName('IsThrallMachineRecipe')]
    IsThrallMachineRecipe : boolean;
    [JsonName('IsRecipeEnabled')]
    IsRecipeEnabled : boolean;
    [JsonName('MapMarkerIcon')]
    MapMarkerIcon : string;
    [JsonName('ExilesJourneyTrigger')]
    ExilesJourneyTrigger : string;
    [JsonName('DLCPackage')]
    DLCPackage : string;
  end;

  CEItemStatModification = class
    public
    [JsonName('RowName')]
    RowName : string;
    [JsonName('Modifications')]
    Modifications : TStringList;
    Owned : Boolean;
    published
    constructor Create;
    destructor Destroy; override;
  end;
	
	CEEquipmentTemplate = class
		public
    [JsonName('RowName')]
    RowName : string;
		public
    [JsonName('MainHand')]
    MainHand : TStringList;
		public
    [JsonName('OffHand')]
    OffHand : TStringList;
		public
    [JsonName('Helmet')]
    Helmet : TStringList;
		public
    [JsonName('Torso')]
    Torso : TStringList;
		public
    [JsonName('Legs')]
    Legs : TStringList;
		public
    [JsonName('Hands')]
    Hands : TStringList;
		public
    [JsonName('Feet')]
    Feet : TStringList;
		public
    [JsonName('Backpack')]
    Backpack : TStringList;
		public
    [JsonName('Durability')]
    Durability : string;
		published
    constructor Create;
    destructor Destroy; override;
	end;

  ptrCEItemStatModification = ^CEItemStatModification;

implementation

  constructor CEItemStatModification.Create;
  begin
    //ShowMessage('Created TStringList!');
    inherited Create;
    Modifications := TStringList.Create;
  end;

  destructor CEItemStatModification.Destroy;
  begin
    //ShowMessage('Destroyed object!');
    Modifications.Free;
    inherited Destroy;
  end;
	
  constructor CEEquipmentTemplate.Create;
  begin
    //ShowMessage('Created TStringList!');
    inherited Create;
    MainHand := TStringList.Create;
		OffHand := TStringList.Create;
		Helmet := TStringList.Create;
		Torso := TStringList.Create;
		Legs := TStringList.Create;
		Hands := TStringList.Create;
		Feet := TStringList.Create;
		Backpack := TStringList.Create;
  end;

  destructor CEEquipmentTemplate.Destroy;
  begin
    //ShowMessage('Destroyed object!');
    MainHand.Free;
		OffHand.Free;
		Helmet.Free;
		Torso.Free;
		Legs.Free;
		Hands.Free;
		Feet.Free;
		Backpack.Free;
    inherited Destroy;
  end;

end.
