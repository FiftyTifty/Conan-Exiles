    DamageStaminaHeavy_OnHit : FixedUInt;
    [JsonName('DamageHealthLight_OnBlock')]
    DamageHealthLight_OnBlock : FixedUInt;
    [JsonName('DamageHealthHeavy_OnBlock')]
    DamageHealthHeavy_OnBlock : FixedUInt;
    [JsonName('DamageStaminaLight_OnBlock')]
    DamageStaminaLight_OnBlock : FixedUInt;
    [JsonName('DamageStaminaHeavy_OnBlock')]
    DamageStaminaHeavy_OnBlock : FixedUInt;
    [JsonName('HarvestDamage')]
    HarvestDamage : FixedUInt;
    [JsonName('WeaponUsage')]
    WeaponUsage : FixedUInt;
    [JsonName('MaxAttackReach')]
    MaxAttackReach : FixedUInt;
    [JsonName('MinAttackReach')]
    MinAttackReach : FixedUInt;
    [JsonName('MinAttackDistance')]
    MinAttackDistance : FixedUInt;
    [JsonName('WeaponEffectWidth')]
    WeaponEffectWidth : FixedUInt;
    [JsonName('WeaponEffectHeight')]
    WeaponEffectHeight : FixedUInt;
    [JsonName('ItemTier')]
    ItemTier : FixedUInt;
    [JsonName('ArmorPen')]
    ArmorPen : FixedUInt;
    [JsonName('DamageConcussiveLightOnHit')]
    DamageConcussiveLightOnHit : FixedUInt;
    [JsonName('DamageConcussiveHeavyOnHit')]
    DamageConcussiveHeavyOnHit : FixedUInt;
    [JsonName('DamageConcussiveLightOnBlock')]
    DamageConcussiveLightOnBlock : FixedUInt;
    [JsonName('DamageConcussiveHeavyOnBlock')]
    DamageConcussiveHeavyOnBlock : FixedUInt;
    [JsonName('DamageTier')]
    DamageTier : string;
    [JsonName('CoolDownTime')]
    CoolDownTime : FixedUInt;
    [JsonName('ReuseTime')]
    ReuseTime : FixedUInt;
    [JsonName('SelectionWeight')]
    SelectionWeight : FixedUInt;
    [JsonName('KnockbackOffenseBasic')]
    KnockbackOffenseBasic : FixedUInt;
    [JsonName('KnockbackOffenseSpecial')]
